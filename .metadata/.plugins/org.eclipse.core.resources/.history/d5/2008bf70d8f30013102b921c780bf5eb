#ifndef DATAMEDICO_H_
#define DATAMEDICO_H_

#include <set>
#include "DataUsuario.h"
#include "DataConsulta"


class DataMedico {
	private:
		DataUsuario* usuario;
		set<DataConsulta*> consultas;
	public:
		DataMedico();
		DataMedico(DataUsuario*,DataConsulta*);
		virtual ~DataMedico();
		DataUsuario* getUsuario();
		set<DataConsulta*> getConsultas();
};

#endif /* DATAMEDICO_H_ */
