#include "ManejadorMedicos.h"
#include "Usuario.h"

int main(){

	ManejadorMedicos* mm = ManejadorMedicos::getInstance();
	mm->show();

	Fecha* f = new Fecha(22,5,1958);
	Usuario * u = new Usuario(1234,"Shirley","Nieto",F,true,*f);
	Medico* m = new Medico(u);
	u->agregarRol(m);

	mm->agregarMedico(m);
	mm->show();

	f = new Fecha(15,7,1992);
	u = new Usuario(1234,"Pablo","Martinez",M,true,*f);
	m = new Medico(u);
	u->agregarRol(m);

	mm->agregarMedico(m);
	mm->show();

	return 0;

}



