#include "Rol.h"

int main(){

	Fecha nacimiento = Fecha(1,2,3);
	Usuario* observador = new Usuario(1234,"nombre","apellido",M,false,nacimiento);
	Socio* s = new Socio(observador);
	observador->agregarRol(s);

	Usuario* medico = new Usuario(9876,"nombre","apellido",F, true,nacimiento);
	Medico* m = new Medico(medico);
	medico->agregarRol(m);


	return 0;
}
